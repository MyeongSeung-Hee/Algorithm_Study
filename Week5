#5주차
#1978
N = int(input())

PrimeNum = list(map(int, input().split(" ")))
HowManyPrime = 0

def IsItPrime(n):
    dividend = 0
    if n == 1:
        return False
    elif n == 2:
        return True
    for i in range (2, n):
        if n % i == 0:
            return False
    return True

for i in PrimeNum:
    if IsItPrime(i):
        HowManyPrime += 1

print(HowManyPrime)
